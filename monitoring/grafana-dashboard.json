{
  "title": "Concert-U Overview",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "5s",
  "panels": [
    {
      "type": "timeseries",
      "title": "HTTP p95 (ticketing)",
      "datasource": "Prometheus",
      "gridPos": { "x": 0, "y": 0, "w": 12, "h": 6 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_server_requests_seconds_bucket{job=\"ticketing\"}[1m])) by (le))"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "HTTP p95 (queue)",
      "datasource": "Prometheus",
      "gridPos": { "x": 12, "y": 0, "w": 12, "h": 6 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_server_requests_seconds_bucket{job=\"queue\"}[1m])) by (le))"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Hikari Active Connections (ticketing)",
      "datasource": "Prometheus",
      "gridPos": { "x": 0, "y": 6, "w": 12, "h": 6 },
      "targets": [
        {
          "expr": "sum(hikaricp_connections_active{job=\"ticketing\"})"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "JVM Heap Used (ticketing)",
      "datasource": "Prometheus",
      "gridPos": { "x": 12, "y": 6, "w": 12, "h": 6 },
      "targets": [
        {
          "expr": "sum(jvm_memory_used_bytes{job=\"ticketing\", area=\"heap\"})"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "MySQL Connections",
      "datasource": "Prometheus",
      "gridPos": { "x": 0, "y": 12, "w": 12, "h": 6 },
      "targets": [
        {
          "expr": "mysql_global_status_threads_connected"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Redis Ops/sec",
      "datasource": "Prometheus",
      "gridPos": { "x": 12, "y": 12, "w": 12, "h": 6 },
      "targets": [
        {
          "expr": "rate(redis_commands_processed_total[1m])"
        }
      ]
    }
  ]
}
